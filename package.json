{
  "name": "seleniumscript-portal",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "build": "cross-env NODE_ENV=development webpack --config webpack.config.prod.js",
    "audit:prod": "yarn audit --groups dependencies",
    "lint": "eslint . --ext=js,jsx --fix",
    "lint:ci": "eslint . --ext=js,jsx",
    "test": "yarn test:client && yarn test:server",
    "test:ci": "yarn test:client -- --runInBand && yarn test:server -- --runInBand",
    "test:client": "jest --config=src/jest.config.json",
    "test:server": "jest --config=server/jest.config.json",
    "test:watch": "jest --watch --config=src/jest.config.json",
    "start:dev": "cross-env NODE_ENV=development node server",
    "start:nf": "SEPARATE_UI_AND_SERVER=true nf start"
  },
  "dependencies": {
    "@catapulthealth/catapult-css": "^1.2.0",
    "@catapulthealth/catapult-dialog": "^1.3.0",
    "@catapulthealth/catapult-server-utils": "^1.36.0",
    "axios": "^0.21.1",
    "babel-plugin-react-css-modules": "^5.2.6",
    "connect-history-api-fallback": "^1.6.0",
    "cross-env": "^6.0.3",
    "dotenv": "^8.2.0",
    "immer": "^8.0.1",
    "jsonwebtoken": "^8.3.0",
    "moment": "^2.23.0",
    "prop-types": "^15.6.1",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-router-dom": "^5.2.0",
    "react-transition-group": "^4.3.0",
    "superstruct": "^0.8.2"
  },
  "devDependencies": {
    "@babel/core": "^7.7.7",
    "@babel/runtime": "^7.7.7",
    "axios-mock-adapter": "^1.19.0",
    "babel-eslint": "^10.0.3",
    "babel-jest": "^24.9.0",
    "babel-loader": "^8.0.6",
    "babel-plugin-add-react-displayname": "^0.0.5",
    "babel-preset-airbnb": "^4.4.0",
    "css-loader": "^3.4.0",
    "cssnano": "^4.1.8",
    "eslint": "^7.32.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-prettier": "^6.7.0",
    "eslint-import-resolver-webpack": "^0.12.0",
    "eslint-plugin-import": "^2.19.1",
    "eslint-plugin-jest": "^23.1.1",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-react": "^7.17.0",
    "file-loader": "^5.0.2",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^24.9.0",
    "postcss-color-function": "^4.0.1",
    "postcss-custom-properties": "^6.3.1",
    "postcss-import": "^12.0.1",
    "postcss-loader": "^3.0.0",
    "pre-commit": "^1.2.2",
    "prettier": "^1.19.1",
    "react-test-renderer": "^16.12.0",
    "redux-mock-store": "^1.5.4",
    "shipit-cli": "^3.0.0",
    "shipit-deploy": "^2.5.1",
    "style-loader": "^0.23.1",
    "supertest": "^4.0.2",
    "terser-webpack-plugin": "^2.3.1",
    "webpack": "4.41.4",
    "webpack-cli": "^3.3.10",
    "webpack-dev-middleware": "^3.7.2",
    "webpack-dev-server": "^3.10.1",
    "webpack-hot-middleware": "^2.22.3"
  },
  "pre-commit": [
    "audit:prod",
    "lint:ci"
  ]
}
